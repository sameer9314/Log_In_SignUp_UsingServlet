<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LogInRegistrationServlet</display-name>
  <welcome-file-list>
    <welcome-file>/home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>loginprocess</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.LogInServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginprocess</servlet-name>
    <url-pattern>/loginsuccessfull</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup-successful</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.LogOutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.PreLogInservlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>msignup</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.PreSignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>msignup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>resignup</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.ResignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>resignup</servlet-name>
    <url-pattern>/resignup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>loginUnsuccess</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.LogInUnSuccessServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginUnsuccess</servlet-name>
    <url-pattern>/loginUnsuccess</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>logInFilter</filter-name>
    <filter-class>com.bridgelabzservlet.controller.AuthenticateFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>logInFilter</filter-name>
    <url-pattern>/loginsuccessfull</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>signUpFilter</filter-name>
    <filter-class>com.bridgelabzservlet.controller.SignUpFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>signUpFilter</filter-name>
    <url-pattern>/signup-successful</url-pattern>
  </filter-mapping>
  
  <!-- Pre Forget Password -->
  <servlet>
    <servlet-name>forget-password</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.ForgetPasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>forget-password</servlet-name>
    <url-pattern>/forget-password</url-pattern>
  </servlet-mapping>
  
   <!-- Post Forget Password -->
  <servlet>
    <servlet-name>password-recover</servlet-name>
    <servlet-class>com.bridgelabzservlet.controller.PostForgetPasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>password-recover</servlet-name>
    <url-pattern>/password-recover</url-pattern>
  </servlet-mapping>
</web-app>